<Page x:Class="Evidence_server.UserInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Evidence_server"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="UserInfo">

    <StackPanel>
        <Button x:Name="Back" Click="Back_Click">Back</Button>
        <Button x:Name="Edit" Click="Edit_Click">Edit</Button>
        <Button x:Name="Delete" Click="Delete_Click">Delete</Button>
        <Label x:Name="name"></Label>
        <Label x:Name="gender"></Label>
        <Label x:Name="birth_num"></Label>
        <Label x:Name="birth"></Label>
    </StackPanel>
</Page>
